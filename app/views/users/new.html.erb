<h1> Sign up</h1>
<%= form_for @user do |f| %>
  <%= f.label "Name" %>
  <%= f.text_field :name %>
  <%= f.label "Email" %>
  <%= f.text_field :email %>
  <%= f.label "Password" %>
  <%= f.password_field :password %>
  <%= f.label :password_confirmation %>
  <%= f.password_field :password_confirmation %>
  <%= f.submit "Sign Up" %>
<% end %>
